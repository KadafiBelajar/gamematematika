{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "VERCEL": "1"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 10,
      "includeFiles": "templates/**"
    }
  }
}